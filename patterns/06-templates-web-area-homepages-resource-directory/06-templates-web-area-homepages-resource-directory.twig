{% set content %}
  {% include '@components/page-title/page-title.twig' with {
    'page_title': title
  } %}

  {% block messages %}
    {{ messages }}
  {% endblock %}

  {% block local_tasks %}
    {{ tasks }}
  {% endblock %}

  {% include '@layouts/resource-directory/resource-directory.twig' with {
    'has_sidebar': has_sidebar,
    'main_top': main_top,
    'num_of_cols_a': num_of_cols_a,
    'a1': a1,
    'a2': a2,
    'num_of_cols_b': num_of_cols_b,
    'b1': b1,
    'b2': b2,
    'num_of_cols_c': num_of_cols_c,
    'c1': c1,
    'c2': c2,
    'main_bottom': main_bottom,
    'sidebar': sidebar,
  } %}
{% endset %}

{% embed '@layouts/page/page.twig' with {
  'has_header': has_header,
  'has_footer': has_footer,
} %}
  {% block info %}
    {{ header_info }}
  {% endblock %}

  {% block contact %}
    {{ contact_link }}
  {% endblock %}

  {% block content %}
    <article class="article">
      {{ content }}
    </article>
  {% endblock %}

  {% block footer %}
    {{ footer }}
  {% endblock %}
{% endembed %}
